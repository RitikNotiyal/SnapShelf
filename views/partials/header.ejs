    <% var loggedIn = typeof loggedIn !== "undefined" ? loggedIn : true %> 
    <% if(loggedIn) { %>
    <div class="flex justify-between items-center">
    <ul class="flex space-x-10">
        <li>
        <a href="/shop" class="text-gray-700 hover:text-gray-900">Shop</a>
        </li>
        <li>
        <a href="/users/account" class="text-gray-700 hover:text-gray-900"
            >My Account</a
        >
        </li>
        <li>
        <a href="/users/logout" class="text-red-700 hover:text-red-900"
            >Log Out</a
        >
        </li>
        <li>
        <a href="/cart" class="text-gray-700 hover:text-gray-900">Cart</a>
        </li>
    </ul>
    </div>
    <% } %>
</nav>
</body>
</html>
